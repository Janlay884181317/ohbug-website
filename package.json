{
  "name": "ohbug-website",
  "description": "Ohbug's website and documentation.",
  "version": "1.0.0",
  "private": true,
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript",
    "react",
    "docs",
    "documents",
    "document"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "ahooks": "^2.9.6",
    "algoliasearch": "^4.8.5",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.32.3",
    "gatsby-plugin-algolia": "^0.16.3",
    "gatsby-plugin-less": "^4.7.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-nprogress": "^2.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-typography": "^2.12.0",
    "gatsby-source-filesystem": "^2.11.0",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-highlight": "^0.13.0",
    "react-instantsearch-dom": "^6.9.0",
    "react-spring": "^8.0.27",
    "react-typography": "^0.16.19",
    "react-use-gesture": "^9.0.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19"
  },
  "devDependencies": {
    "@chenyueban/lint": "^0.1.4",
    "@chenyueban/tsconfig": "^0.1.3",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-plugin-root-import": "^6.6.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.4",
    "typescript": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
