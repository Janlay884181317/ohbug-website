{
  "extends": "@chenyueban/tsconfig",
  "compilerOptions": {
    "lib": ["DOM", "dom.iterable", "ESNEXT"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*"]
}
